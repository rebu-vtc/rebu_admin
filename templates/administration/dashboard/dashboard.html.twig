{% extends "base.administration.html.twig" %}
{% block title %}Dashboard{% endblock %}
{% block stylesheets_position_order_fixed_1 %}
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet"
        href="{{ asset('assets/back-office/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css') }}">
    <!-- iCheck -->
    <link rel="stylesheet" href="{{ asset('assets/back-office/plugins/icheck-bootstrap/icheck-bootstrap.min.css') }}">
    <!-- JQVMap -->
    <link rel="stylesheet" href="{{ asset('assets/back-office/plugins/jqvmap/jqvmap.min.css') }}">
{% endblock %}
{% block stylesheets_position_order_fixed_2 %}
    <!-- Daterange picker -->
    <link rel="stylesheet" href="{{ asset('assets/back-office/plugins/daterangepicker/daterangepicker.css') }}">
    <!-- summernote -->
    <link rel="stylesheet" href="{{ asset('assets/back-office/plugins/summernote/summernote-bs4.min.css') }}">
{% endblock %}
{% block stylesheets %}
{% endblock %}
{% block body %}
    {# Contenu du dashboard Ã  mettre ici. #}
    <!-- Content Header (Page header) -->
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">Dashboard</h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item">
                            <i class="fas fa-tachometer-alt ariane-icon-margin"></i>
                            <a href="{{ path('dashboard') }}" class="dashboard-default" disabled>Dashboard</a>
                        </li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    <section class="content">
        <div class="container-fluid">
            <!-- Small boxes (Stat box) -->
            <div class="row">
                {% for flash_error in app.flashes('success') %}
                    <div class="alert alert-success" role="alert">{{ flash_error }}</div>
                {% endfor %}
                <h1>Dashboard en construction...</h1>
            </div>
        </div>
    </section>
{% endblock %}
{% block javascripts %}
    <!-- jQuery UI 1.11.4 -->
    <script src="{{ asset('assets/back-office/plugins/jquery-ui/jquery-ui.min.js') }}"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button)
    </script>
    <!-- ChartJS -->
    <script src="{{ asset('assets/back-office/plugins/chart.js/Chart.min.js') }}"></script>
    <!-- Sparkline -->
    <script src="{{ asset('assets/back-office/plugins/sparklines/sparkline.js') }}"></script>
    <!-- JQVMap -->
    <script src="{{ asset('assets/back-office/plugins/jqvmap/jquery.vmap.min.js') }}"></script>
    <script src="{{ asset('assets/back-office/plugins/jqvmap/maps/jquery.vmap.usa.js') }}"></script>
    <!-- jQuery Knob Chart -->
    <script src="{{ asset('assets/back-office/plugins/jquery-knob/jquery.knob.min.js') }}"></script>
    <!-- daterangepicker -->
    <script src="{{ asset('assets/back-office/plugins/moment/moment.min.js') }}"></script>
    <script src="{{ asset('assets/back-office/plugins/daterangepicker/daterangepicker.js') }}"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="{{ asset('assets/back-office/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js') }}">
    </script>
    <!-- Summernote -->
    <script src="{{ asset('assets/back-office/plugins/summernote/summernote-bs4.min.js') }}"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="{{ asset('assets/back-office/dist/js/demo.js') }}"></script>
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="{{ asset('assets/back-office/dist/js/pages/dashboard.js') }}"></script>
{% endblock %}